<%if logged_in? %>
<% if  attending_already?(event)%>
        <%= link_to "Attend", event_event_attendees_path(event.id), method: :create, class:"btn btn-warning" %>
    <%else%>
  <%= link_to "Cancel ", event_event_attendees_path(event.id), method: :delete, class:"btn btn-warning" %>
<% end  %>
 <% end %>
<h4 class="mt-3" >Who is going?</h4>
<ul class="py-1">
<% event.attendees.find_each do |u| %>
    <li class=" "><%= u.username %> </li>
<% end %>
</ul>